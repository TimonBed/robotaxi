{
  "name": "robotaximap",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack -p 3001",
    "build": "next build",
    "vercel-build": "(prisma migrate deploy || prisma db push) && prisma db seed && next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "tsx prisma/seed.ts",
    "prisma:seed": "prisma db seed",
    "add:geofences": "tsx scripts/add-geofences.ts",
    "add:waymo": "tsx scripts/add-waymo-geofences.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@turf/turf": "^7.0.0",
    "argon2": "0.44.0",
    "mapbox-gl": "^3.6.0",
    "maplibre-gl-draw": "^1.0.3",
    "maplibre-gl": "5.9.0",
    "next": "^15.5.6",
    "next-auth": "^4.24.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.23.8",
    "@prisma/client": "^6.17.1"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^15.5.6",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-next": "^15.5.6",
    "postcss": "^8.4.47",
    "prisma": "^6.17.1",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3"
  }
}
