{
  "name": "robotaximap",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "docker compose up -d && next dev -p 3001",
    "build": "next build",
    "vercel-build": "(prisma migrate deploy || prisma db push) && prisma db seed && next build",
    "start": "next start -p 3001",
    "lint": "next lint",
    "postinstall": "prisma generate",
    "prisma:generate": "prisma generate",
    "prisma:deploy": "prisma migrate deploy",
    "prisma:migrate": "prisma migrate dev --name init",
    "seed": "tsx prisma/seed.ts",
    "prisma:seed": "prisma db seed",
    "db:up": "docker compose up -d db",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f db",
    "db:reset": "docker compose down -v && docker compose up -d db && prisma migrate reset --force && prisma db seed",
    "add:geofences": "tsx scripts/add-geofences.ts",
    "add:waymo": "tsx scripts/add-waymo-geofences.ts"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.5",
    "@prisma/client": "^6.17.1",
    "@turf/turf": "^7.0.0",
    "argon2": "0.44.0",
    "mapbox-gl": "^3.6.0",
    "maplibre-gl": "5.9.0",
    "maplibre-gl-draw": "^1.0.3",
    "next": "^16.0.0",
    "next-auth": "^5.0.0-beta.29",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.0.0",
    "@types/node": "^20.11.30",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-config-next": "^16.0.0",
    "postcss": "^8.4.47",
    "prisma": "^6.17.1",
    "tailwindcss": "^3.4.13",
    "tsx": "^4.19.1",
    "typescript": "^5.7.0"
  }
}
