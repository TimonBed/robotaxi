# NextAuth
NEXTAUTH_SECRET=
NEXTAUTH_URL=

# Database
# For development with SQLite (Prisma):
# DATABASE_URL="file:./prisma/dev.db"
# For production on Vercel (use a hosted DB):
# Example Neon Postgres:
# DATABASE_URL="postgresql://user:password@pooled-host:5432/db?sslmode=require"
# Use a DIRECT_URL (non-pooled) so Prisma Migrate can connect directly on deploy
# DIRECT_URL="postgresql://user:password@direct-host:5432/db?sslmode=require"

# Public map key (optional)
NEXT_PUBLIC_MAPTILER_KEY=

# Seed admin credentials (optional; used by prisma/seed.ts)
ADMIN_EMAIL=
ADMIN_PASSWORD=
